  object qryicms_ent_en: TADOQuery
    Connection = DM.ADOConnection1
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'CGT'
        Attributes = [paSigned, paNullable]
        DataType = ftInteger
        Precision = 10
        Size = 4
        Value = Null
      end
      item
        Name = 'Pro'
        Attributes = [paSigned]
        DataType = ftInteger
        Precision = 10
        Size = 4
        Value = Null
      end>
    SQL.Strings = (
      
        'select ITICMS.Origem, ITICMS.Destino, RICMS.Descricao as Descric' +
        'ao_ICMS, RICMS.Aliquota as Aliquota_ICMS, RICMS.Tipo_Recolhiment' +
        'o as Tipo_Recolhimento_ICMS,'
      
        'RICMS.Base_Reduzida as Base_Reduzida_ICMS, RICMS.Percentual_BC_R' +
        'eduzida as Percentual_BC_Reduzida_ICMS, RICMS.Base_Acrescida as ' +
        'Base_Acrescida_ICMS,'
      
        'RICMS.Percentual_BC_Acrescida as Percentual_BC_Acrescida_ICMS, C' +
        'ST.Codigo_Situacao as Codigo_ST_ICMS, CST.Descricao as Descricao' +
        '_ST_ICMS'
      ''
      'from Produto Pro'
      ''
      
        'inner join Tratamento_ICMS TICMS on (Pro.Codigo_Grupo_Tributacao' +
        ' = TICMS.Codigo)'
      
        'inner join Itens_Tratamento_ICMS ITICMS on (TICMS.Codigo = ITICM' +
        'S.Codigo)'
      
        'inner join Regra_ICMS RICMS on (ITICMS.Entrada_Regra_EN = RICMS.' +
        'Codigo)'
      ''
      
        'inner join Cadastro_Situacao_Tributaria CST on (RICMS.Codigo_Sit' +
        'uacao_Tributaria = CST.Codigo)'
      ''
      ''
      'where Pro.Codigo_Grupo_Tributacao = :CGT and Pro.Codigo = :Pro')
