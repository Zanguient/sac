procedure TPedido.Salva_Banco(var Registro: TPedido);
var
  qAux, qaux2: TADOQuery;
begin
  try
    try
      qAux:= TADOQuery.Create(nil);
      begin
        with qAux, SQL do
          begin
            close;
            clear;
            Connection:= dm.ADOConnection1;
            add('Insert into Pedido (Codigo, N_Pedido, N_Pre_Venda, Codigo_Cliente, Codigo_Empresa, Codigo_Veiculo,');
            add('Codigo_Funcionario, Codigo_Tabela_Preco, Codigo_Forma_Pag, Data_Venda, Data_Vencimento, Qtde_Itens, Qtde_Servicos, Total_Produtos, Total_Servicos,');
            add('Total_Desc_Acr, Desc_Acr, Tipo_Desc_Acr, Total_Pedido, ');
            add('Tipo, Status, Status_OS, Valor_Recebido, Troco, Mesclada, Problema_Reclamado, Problema_Constatado, Solucao, DAVC, DAVAnt) values ');
            add('(:Codigo, :N_Pedido, :N_Pre_Venda, :Codigo_Cliente, :Codigo_Empresa, :Codigo_Veiculo, :Codigo_Funcionario, :Codigo_Tabela_Preco,');
            add(':Codigo_Forma_Pag, :Data_Venda, :Data_Vencimento, :Qtde_Itens, :Qtde_Servicos,');
            add(':Total_Produtos, :Total_Servicos, :Total_Desc_Acr, :Desc_Acr, :Tipo_Desc_Acr, :Total_Pedido, ');
            add(':Tipo, :Status, :Status_OS, :Valor_Recebido, :Troco, :Mesclada, :Problema_Reclamado, :Problema_Constatado, :Solucao, :DAVC, :DAVAnt)');
            Parameters.ParamByName('Codigo').Value:= Registro.Cod;
            Parameters.ParamByName('N_Pedido').Value:= Registro.N_Ped;
            Parameters.ParamByName('N_Pre_Venda').Value:= Registro.N_Pre_Ven;
            Parameters.ParamByName('Codigo_Cliente').Value:= Registro.Cod_Cli;
            Parameters.ParamByName('Codigo_Empresa').Value:= Registro.Cod_Empresa;
            Parameters.ParamByName('Codigo_Veiculo').Value:= Registro.Cod_Veiculo;
            Parameters.ParamByName('Codigo_Funcionario').Value:= Registro.Cod_Funcionario;
            Parameters.ParamByName('Codigo_Tabela_Preco').Value:= Registro.Cod_Tabela_Preco;
            Parameters.ParamByName('Codigo_Forma_Pag').Value:= Registro.Cod_Forma_Pag;
            Parameters.ParamByName('Data_Venda').Value:= Registro.Data_Ve;
            Parameters.ParamByName('Data_Vencimento').Value:= Registro.Data_Venc;
            Parameters.ParamByName('Qtde_Itens').Value:= Registro.Qtde_It;
            Parameters.ParamByName('Qtde_Servicos').Value:= Registro.Qtde_Serv;
            Parameters.ParamByName('Total_Produtos').Value:= Registro.Tot_Prod;
            Parameters.ParamByName('Total_Servicos').Value:= Registro.Tot_Serv;
            Parameters.ParamByName('Total_Desc_Acr').Value:= Registro.Tot_Desc_Acr;
            Parameters.ParamByName('Desc_Acr').Value:= Registro.Desc_Acr_Pedido;
            Parameters.ParamByName('Tipo_Desc_Acr').Value:= Tipo_Desc_Acr_Pedido;
            Parameters.ParamByName('Total_Pedido').Value:= Registro.Tot_Ped;
            Parameters.ParamByName('Tipo').Value:= tipo_venda;
            Parameters.ParamByName('Status').Value:= Registro.Stat;

            if (FrmOS <> nil) and (FrmOS.ativo) then
            begin
              if (FrmOS.CBFechar.Checked) then
                Parameters.ParamByName('Status_OS').Value:= 'FECHADO'
              else
                Parameters.ParamByName('Status_OS').Value:= 'ABERTO';
            end
            else if (FrmPedido_Venda <> nil) and (FrmPedido_Venda.ativo) then
            begin
              Parameters.ParamByName('Status_OS').Value:= 'FECHADO';
            end
            else if (FrmPre_Venda <> nil) and (FrmPre_Venda.ativo) then
            begin
              Parameters.ParamByName('Status_OS').Value:= 'FECHADO';
            end
            else if (FrmPedido_Venda_Extra <> nil) and (FrmPedido_Venda_Extra.ativo) then
            begin
              Parameters.ParamByName('Status_OS').Value:= 'FECHADO';
            end
            else
              Parameters.ParamByName('Status_OS').Value:= '';

            Parameters.ParamByName('Valor_Recebido').Value:= Registro.Val_Rec;
            Parameters.ParamByName('Troco').Value:= Registro.Troc;
            Parameters.ParamByName('Mesclada').Value:= Registro.Mesc;
            Parameters.ParamByName('Problema_Reclamado').Value:= Registro.Problema_Rec;
            Parameters.ParamByName('Problema_Constatado').Value:= Registro.Problema_Const;
            Parameters.ParamByName('Solucao').Value:= Registro.Soluc;
            Parameters.ParamByName('DAVC').Value:= Registro.DAVCC;
            Parameters.ParamByName('DAVAnt').Value:= Registro.DAVAntt;
            ExecSQL;
          end;
      end;
    finally
      qAux.Close;
      FreeAndNil(qAux);
      //lrControle:= Insere;
      //UGeral.Mensagem_Padrao(lrControle);
    end;

  except
    on E: Exception do
      begin
        DM.ADOConnection1.RollbackTrans; Impossivel_Excluir_Registro;
      end;
  end;
end;

