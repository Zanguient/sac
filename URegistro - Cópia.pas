procedure TPerfil_Usuario.Excluir;
var
  qAux, qaux2: TADOQuery;
begin
  try
    DM.ADOConnection1.BeginTrans;
    try
      qAux:= TADOQuery.Create(nil);
      qAux2:= TADOQuery.Create(nil);
      with qAux, SQL do
        begin
          close;
          clear;
          Connection:= dm.ADOConnection1;
          add('Delete from Perfil_Usuario where Codigo = :Codigo');
          Parameters.ParamByName('Codigo').Value:= StrToInt(FrmCadastro_Perfil_Usuario.EdtCodigo.Text);
          ExecSQL;
        end;
    finally
      DM.ADOConnection1.CommitTrans;
      lrControle:= Exclui;
      UGeral.Mensagem_Padrao(lrControle);
      qAux.Close;
      FreeAndNil(qAux);
    end;

  except
    on E: Exception do
      begin
        DM.ADOConnection1.RollbackTrans; Impossivel_Excluir_Registro;
      end;
  end;
end;

